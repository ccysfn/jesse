<odoo>
  <data>
    <!-- explicit list view definition -->
    
    <record model="ir.ui.view" id="course_list">
      <field name="name">jyinspur course</field>
      <field name="model">product.template</field>
      <field name="inherit_id" ref="product.product_template_form_view""/>
      <field name="arch" type="xml">
	    <data><xpath expr="//page[@name='general_information']" position="inside">
                    <group string="课程">
                     
                        <field name="c_name"/>
                    </group>
                    </xpath>
                </data>
      </field>
    </record>
    


    <!-- actions opening views on models -->
    
    <record model="ir.actions.act_window" id="course_action_window">
      <field name="name">jyinspur course</field>
      <field name="res_model">product.product</field>
      <field name="view_mode">tree,form</field>
    </record>
    

    <!-- server action to the one above -->
    


    <!-- Top menu item -->
    

    
    <!-- actions -->
    
    <menuitem name="course" id="jyinspur_menu_1_course" parent="jyinspur_menu_config"
              action="course_action_window"/>

  </data>
</odoo>